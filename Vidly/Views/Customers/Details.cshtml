@model Vidly.Models.Customer

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.Name</li>
    @if(Model.Birthday != null)
    {
        <li>Birthday: @String.Format("{0:d}",Model.Birthday)</li>
    }
    
</ul>

